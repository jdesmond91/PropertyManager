<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://propertycloud1.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>propertycloud1.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>propertycloud1</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>False</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$propertycloud1</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="True">
          <Destination Path="Data Source=propertycloud.database.windows.net;Initial Catalog=PropertyAppDB;Persist Security Info=True;User ID=propertyadmin;Password=PropertyApp123" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="PropertyManager.Models.ApplicationDbContext, PropertyManager" MigrationConfiguration="PropertyManager.Migrations.Configuration, PropertyManager" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=propertycloud.database.windows.net;Initial Catalog=PropertyAppDB;Persist Security Info=True;User ID=propertyadmin;Password=PropertyApp123</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>